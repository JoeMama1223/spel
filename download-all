# Download index.html
rm index.html
curl -O http://tinysubversions.com/game/spelunky/index.html

# Download game.html
rm game.html
curl -O http://tinysubversions.com/game/spelunky/game.html

# Style files
rm -r -f css
mkdir css
cd css
curl -O http://tinysubversions.com/game/spelunky/css/style.css
cd ../

# Landing page graphics
rm -r -f images
mkdir images
cd images
curl -O http://tinysubversions.com/game/spelunky/images/bg_tile.png
curl -O http://tinysubversions.com/game/spelunky/images/game_frame.png
curl -O http://tinysubversions.com/game/spelunky/images/logo_2.png
cd ../

# Download Root Directory level files 
rm -r -f html5game
mkdir html5game
cd html5game
curl -O http://tinysubversions.com/game/spelunky/html5game/index.js
curl -O http://tinysubversions.com/game/spelunky/html5game/index_texture_0.png
cd ../

